        "!pip install flask_ngrok\n",
        "import json\n",
        "from flask_ngrok import run_with_ngrok\n",
        "from flask import Flask\n",
        "app = Flask(__name__)\n",
        "run_with_ngrok(app)   #starts ngrok when the app is run\n",
        "@app.route(\"/\")\n",
        "def home():\n",
        "  with open('data.json') as f:\n",
        "    j1=json.load(f)\n",
        "    return j1\n",
        "  \n",
        "app.run()"
      ],
